import junit.framework.TestCase;

import org.junit.Before;
import org.junit.Test;

public class QueueTest extends TestCase {
	private MyQueue myQueue;

	@Test
	public void testIsEmptyOnConstruction() {
		setUp();
		boolean empty = myQueue.isEmpty();
		assertTrue("queue not empty on construction", empty);
	}

	@Before
	protected void setUp() {
		myQueue = new MyQueue();
	}

	@Test
	public void enQueue() {
		myQueue.enqueue("Kevin");
		assertFalse("Queue still empty",myQueue.isEmpty());
	}
	
	@Test
	public void enqueueThenDequeueMatches() {
		fail("test not implmented");
	}

}
